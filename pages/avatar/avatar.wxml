<import src="../we-cropper/dist/we-cropper.wxml" />
<view class="imgDisposeBlock">
    <view class='imgDisposeArea'>
        <template is="we-cropper" data="{{...cropperOpt}}" />
    </view>


    <view class="cropper-wrapper">
        <template is="we-cropper" data="{{...cropperOpt}}" />
        <view class="cropper-buttons">
            <view class='imgDisposeControlLine'>
                <view class='editBtn reelectBtn' bindtap="uploadTap">选择图片</view>
                <view class='editBtn editPerfectBtn' bindtap="getCropperImage">上传头像</view>
            </view>
        </view>
    </view>


</view>